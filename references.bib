@article{perssonWereMacrobenthicChanges1981,
  title = {Were Macrobenthic Changes Induced by Thinning out of Flatfish Stocks in the {{Baltic}} Proper?},
  author = {Persson, Lars-Eric},
  year = {1981},
  month = dec,
  journal = {null},
  volume = {20},
  number = {2},
  pages = {137--152},
  publisher = {{Taylor \& Francis}},
  issn = {0078-5326},
  doi = {10.1080/00785236.1981.10426566},
  abstract = {Possible causes of longterm increase in macrobenthic biomass in the Baltic proper are discussed. Eutrophication by means of nutrient enrichment is shown to be a process of the last three decades. It is assumed that primary production has doubled during this time. Macrobenthic data from the southern Baltic indicate highly increased biomass values above the halocline between the 1920s and 1950s. During the last three decades no biomass increase was found. Because of these facts, eutrophication is rejected as a major cause of increased macrobenthic biomass. Flatfish populations in the southern Baltic were very dense before the 1920s. This resulted in stunted growth due to strong competition for food. After heavy exploitation in the 1920s and the early 1930s, these stocks were thinned out and showed significantly higher growth rates due to the reduced competition. In the late 1930s the catches of cod increased markedly. Based on evidence from studies of predator-prey relationships it is proposed that the increased macrobenthic biomass values are due to the decreased grazing pressure following the exploitation of the flatfishes. Furthermore, it is assumed that the increase of the stock of cod in the 1930s was due to reduced food competition between young cod and flatfishes, especially the dab.},
  file = {files/2683/00785236.1981.html}
}

@article{petcheyBodysizeDistributionsSizespectra2010,
  title = {Body-Size Distributions and Size-Spectra: Universal Indicators of Ecological Status?},
  author = {Petchey, O. L. and Belgrano, A.},
  year = {2010},
  journal = {Biology Letters},
  volume = {6},

@article{ngPredatorStomachContents2021,
  title = {Predator Stomach Contents Can Provide Accurate Indices of Prey Biomass},
  author = {Ng, Elizabeth L and Deroba, Jonathan J and Essington, Timothy E and Gr{\"u}ss, Arnaud and Smith, Brian E and Thorson, James T},

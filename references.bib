@article{cavraroBodyConditionFish2019,
  title = {Body Condition in Fish as a Tool to Detect the Effects of Anthropogenic Pressures in Transitional Waters},
  author = {Cavraro, Francesco and Bettoso, Nicola and Zucchetta, Matteo and D'Aietti, Alessandro and Faresi, Lisa and Franzoi, Piero},

@article{andersenSizeStructureNot2015,
  title = {Size Structure, Not Metabolic Scaling Rules, Determines Fisheries Reference Points},
  author = {Andersen, Ken H and Beyer, Jan E},
  year = {2015},
  month = mar,
  journal = {Fish Fish},
  volume = {16},
  number = {1},
  pages = {1--22},
  issn = {14672960},
  doi = {10.1111/faf.12042},
  abstract = {Impact assessments of fishing on a stock require parameterization of vital rates: growth, mortality and recruitment. For `data-poor' stocks, vital rates may be estimated from empirical size-based relationships or from life-history invariants. However, a theoretical framework to synthesize these empirical relations is lacking. Here, we combine life-history invariants, metabolic scaling and size-spectrum theory to develop a general size- and trait-based theory for demography and recruitment of exploited fish stocks. Important concepts are physiological or metabolic scaled mortalities and flux of individuals or their biomass to size. The theory is based on classic metabolic relations at the individual level and uses asymptotic size W{$\infty$} as a trait. The theory predicts fundamental similarities and differences between small and large species in vital rates and response to fishing. The central result is that larger species have a higher egg production per recruit than small species. This means that density dependence is stronger for large than for small species and has the consequence that fisheries reference points that incorporate recruitment do not obey metabolic scaling rules. This result implies that even though small species have a higher productivity than large species their resilience towards fishing is lower than expected from metabolic scaling rules. Further, we show that the fishing mortality leading to maximum yield per recruit is an ill-suited reference point. The theory can be used to generalize the impact of fishing across species and for making demographic and evolutionary impact assessments of fishing, particularly in data-poor situations.},
  langid = {english},
  file = {files/1952/Andersen and Beyer - 2015 - Size structure, not metabolic scaling rules, deter.pdf}
}

@article{andersenTheoreticalFoundationsSize2016,
  title = {The Theoretical Foundations for Size Spectrum Models of Fish Communities},
  author = {Andersen, Ken Haste. and Jacobsen, Nis S and Life, Ocean and Resources, Aquatic and Castle, Charlottenlund and Security, Global Food and Road, Lisburn and Bt, Belfast and Ireland, Northern and Farnsworth, K D},
  year = {2016},
  journal = {Canadian Journal of Fisheries and Aquatic Sciences},
  volume = {73},
  number = {4},
  pages = {1--47},
  issn = {0706-652X},
  doi = {10.1139/cjfas-2015-0230},
  abstract = {Size spectrum models have emerged from 40 years of basic research on how\$\textbackslash backslash\$nbody size determines individual physiology and structures marine\$\textbackslash backslash\$ncommunities. They are based on commonly accepted assumptions and have a\$\textbackslash backslash\$nlow parameter set, making them easy to deploy for strategic\$\textbackslash backslash\$necosystem-oriented impact assessment of fisheries. We describe the\$\textbackslash backslash\$nfundamental concepts in size-based models about food encounter and the\$\textbackslash backslash\$nbioenergetics budget of individuals. Within the general framework, three\$\textbackslash backslash\$nmodel types have emerged that differ in their degree of complexity: the\$\textbackslash backslash\$nfood-web, the trait-based, and the community models. We demonstrate the\$\textbackslash backslash\$ndifferences between the models through examples of their response to\$\textbackslash backslash\$nfishing and their dynamic behavior. We review implementations of size\$\textbackslash backslash\$nspectrum models and describe important variations concerning the\$\textbackslash backslash\$nfunctional response, whether growth is food-dependent or fixed, and the\$\textbackslash backslash\$ndensity dependence imposed on the system. Finally, we discuss challenges\$\textbackslash backslash\$nand promising directions.},
  file = {files/1422/Andersen et al 2015 Can. J. Fish. Aquat. Sci.pdf}
}


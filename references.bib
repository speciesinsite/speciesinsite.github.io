@article{linOptimumTemperatureGrowth2008,
  title = {Optimum Temperature for the Growth Performance of Juvenile Orange-Spotted Grouper ({{{\emph{Epinephelus}}}}{\emph{ Coioides}} {{H}}.)},
  author = {Lin, Xiangzhi and Xie, Shouqi and Su, Yongquan and Cui, Yibo},

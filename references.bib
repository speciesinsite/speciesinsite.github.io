@article{paquetteBioticInteractionsAre2021,
  title = {Biotic Interactions Are More Often Important at Species' Warm versus Cool Range Edges},
  author = {Paquette, Alexandra and Hargreaves, Anna L.},
  year = {2021},
  journal = {Ecology Letters},
  volume = {24},
  number = {11},
  pages = {2427--2438},
  issn = {1461-0248},
  doi = {10.1111/ele.13864},
  abstract = {Predicting which ecological factors constrain species distributions is a fundamental ecological question and critical to forecasting geographic responses to global change. Darwin hypothesised that abiotic factors generally impose species' high-latitude and high-elevation (typically cool) range limits, whereas biotic interactions more often impose species' low-latitude/low-elevation (typically warm) limits, but empirical support has been mixed. Here, we clarify three predictions arising from Darwin's hypothesis and show that previously mixed support is partially due to researchers testing different predictions. Using a comprehensive literature review (885 range limits), we find that biotic interactions, including competition, predation and parasitism, contributed to {$>$}60\% of range limits and influenced species' warm limits more often than cool limits. Abiotic factors contributed more often than biotic interactions to cool range limits, but temperature contributed frequently to both cool and warm limits. Our results suggest that most range limits will be sensitive to climate warming, but warm-limit responses in particular will depend strongly on biotic interactions.},
  langid = {english},
  keywords = {competition,latitudinal gradients,literature review,pathogens,predation,range limits,species distributions,species interactions,temperature},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/ele.13864},
  file = {files/4092/Paquette and Hargreaves - 2021 - Biotic interactions are more often important at sp.pdf;files/4093/ele.html}
}


@article{hedgesMetaAnalysisResponseRatios1999,
  title = {The {{Meta-Analysis}} of {{Response Ratios}} in {{Experimental Ecology}}},
  author = {Hedges, Larry V. and Gurevitch, Jessica and Curtis, Peter S.},
  year = {1999},
  journal = {Ecology},
  volume = {80},
  number = {4},
  pages = {1150--1156},
  issn = {1939-9170},
  doi = {10.1890/0012-9658(1999)080[1150:TMAORR]2.0.CO;2},
  abstract = {Meta-analysis provides formal statistical techniques for summarizing the results of independent experiments and is increasingly being used in ecology. The response ratio (the ratio of mean outcome in the experimental group to that in the control group) and closely related measures of proportionate change are often used as measures of effect magnitude in ecology. Using these metrics for meta-analysis requires knowledge of their statistical properties, but these have not been previously derived. We give the approximate sampling distribution of the log response ratio, discuss why it is a particularly useful metric for many applications in ecology, and demonstrate how to use it in meta-analysis. The meta-analysis of response-ratio data is illustrated using experimental data on the effects of increased atmospheric CO2 on plant biomass responses.},
  langid = {english},
  keywords = {effect size,elevated CO2,meta-analysis in experimental ecology,meta-analysis of response ratios,plant biomass responses,research synthesis,response ratio,statistical techniques},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1890/0012-9658\%281999\%29080\%5B1150\%3ATMAORR\%5D2.0.CO\%3B2},
  file = {files/3952/Hedges et al. - 1999 - The Meta-Analysis of Response Ratios in Experiment.pdf;files/3951/0012-9658(1999)080[1150TMAORR]2.0.html}
}

@article{heiboLATITUDINALVARIATIONLIFEHISTORY2005,
  title = {{{LATITUDINAL VARIATION IN LIFE-HISTORY TRAITS IN EURASIAN PERCH}}},
  author = {Heibo, Erik and Magnhagen, Carin and V{\o}llestad, Leif Asbj{\o}rn},
  year = {2005},
  month = dec,
  journal = {Ecology},
  volume = {86},
  number = {12},
  pages = {3377--3386},
  issn = {0012-9658},
  doi = {10.1890/04-1620},
  abstract = {Few studies have examined multiple life-history traits across a latitudinal gradient to test whether variation in growth rate and mortality schedules induces trends predicted by life-history theory. We collected data for the following life-history traits for 75 Eurasian perch (Perca fluviatilis) populations: growth coefficient (K ) and asymptotic body length (L{$\varrho$}) from the von Bertalanffy growth model, size at ages one and two years, specific juvenile growth rate, instantaneous adult and juvenile mortality rates, life span, age and length at maturity, and reproductive life span and investment. All life-history traits except L{$\varrho$} were significantly correlated with latitude. In general, growth rates, mortality rates, and reproductive investment decreased with latitude, whereas age at maturity, size at maturity, and life span increased with latitude. Populations could be grouped into two categories based on variation in L{$\varrho$}: stunted (small sized) vs. piscivorous (large sized). Four trait\textendash latitude relationships differed between these two types: the growth coefficient (K ) and the juvenile growth rate were larger, and age and length at maturity were lower in the stunted populations compared with piscivorous populations. Perch from southern populations tend to grow fast and experience high juvenile and adult mortality rates. As predicted from life-history theory, this selects for an early age and small size at maturity and relatively large investment in reproduction. The opposite pattern was found for northern populations. Key words: latitudinal cline; life-history variation; trade-offs; Perca fluviatilis L.; perch.},
  langid = {english},
  file = {files/1169/Heibo et al. - 2005 - LATITUDINAL VARIATION IN LIFE-HISTORY TRAITS IN EU.pdf}
}


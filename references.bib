@article{sandstromThreeDecadesSwedish2005,
  title = {Three {{Decades}} of {{Swedish Experience Demonstrates}} the {{Need}} for {{Integrated Long-Term Monitoring}} of {{Fish}} in {{Marine Coastal Areas}}},

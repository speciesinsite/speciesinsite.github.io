@article{abramsWhenDoesGreater2009,
  title = {When Does Greater Mortality Increase Population Size? The Long History and Diverse Mechanisms Underlying the Hydra Effect},
  author = {Abrams, P A.},
  year = {2009},
  journal = {Ecology Letters},
  volume = {12},
  number = {5},
  pages = {462--474},
  issn = {1461023X},
  doi = {10.1111/j.1461-0248.2009.01282.x},
  abstract = {The phenomenon of a population increasing in response to an increase in its per-capita mortality rate has recently been termed the ?hydra effect?. This article reviews and unifies previous work on this phenomenon. Some discrete models of density-dependent growth were shown to exhibit hydra effects in 1954, but the topic was then ignored for decades. Here the history of research on the hydra effect is reviewed, and the key factors producing it are explored. Mortality that precedes overcompensatory density dependence always has the potential to produce hydra effects. Even when mortality follows density dependence, hydra effects may occur in unstable systems due to changes in the amplitude and{$\fracslash$} or form of population cycles. An increase in resource productivity due to lower consumption rates following increased consumer mortality can also produce a hydra effect. Lower consumption can come about as the result of increased satiation of the consumers or changes in behaviour of either consumer or resource species that reduce the mean attack rate. Changes in species composition of a resource community may also decrease the average attack rate. Population structure can promote hydra effects by allowing separation of the timing of density dependence and mortality, although stage-specific density dependence usually decreases hydra effects.},
  pmid = {19220393},
  keywords = {Consumer-resource system,Hydra effects,Instability,Mortality,Overcompensation,Population size,Predation}
}

@techreport{adillBiologiskRecipientkontrollVid2013,
  type = {{Rapport}},
  title = {{Biologisk recipientkontroll vid Forsmarks k\"arnkraftverk (in Swedish)}},
  author = {Adill, Anders and Mo, Kerstin and Sevastik, Alf and Olsson, Jens and Bergstr{\"o}m, Lena},
  year = {2013},
  number = {2013:19},
  address = {{\"Oregrund}},
  abstract = {Denna rapport sammanfattar l\aa ngsiktiga och p\aa g\aa ende trender ikustvattenmilj\"on utanf\"or Forsmarks k\"arnkraftverk fr\aa n mitten av 1970-taletfram till och med \aa r 2012. Resultaten baseras p\aa{} de f\"altunders\"okningar somutf\"ors inom k\"arnkraftverkets kontrollprogram f\"or att f\"olja effekter avkylvattenanv\"andning p\aa{} fisk, f\aa gel och bottenfauna.Vid Forsmarks k\"arnkraftverk pumpas stora m\"angder brackvatten genomkraftverket f\"or att kyla kondensorerna. Efter anv\"andning pumpas detanv\"anda kylvattnet ut i n\"arrecipienten, som f\aa r en temperaturh\"ojning medcirka 7-9\textdegree C. Kylvattenhanteringen har en direkt milj\"op\aa verkan genom attdet havsvatten som anv\"ands inneh\aa ller levande organismer som dras med ini systemet, eller filtreras bort vid intaget och d\"or. Det uppv\"armda kylvattensom n\aa r n\"arrecipienten har d\"artill effekter p\aa{} djurens fysiologi, f\"odotillg\aa ngoch beteendem\"onster, vilket i sin tur kan p\aa verka deras tillv\"axt ochreproduktion. Dessa f\"or\"andringar \"ar v\"al dokumenterade under de \aa r somkontrollprogrammet har p\aa g\aa tt, framf\"orallt n\"ar det g\"aller fisk. I rapportenpresenteras trender i utvecklingen \"over tid hos fisk, bottenfauna och f\aa gel.Data j\"amf\"ors d\"ar det \"ar m\"ojligt med motsvarande trender i referensomr\aa densamt i andra delar av Bottenhavet och \"Ostersj\"on.En stor del av de f\"or\"andringar som observerats i Forsmarks sk\"arg\aa rdunder senare \aa r kan sannolikt sammankopplas med kylvattenutfl\"odet, isynnerhet efter \aa r 2004 n\"ar det galler som tidigare hindrade fiskar fr\aa n attvandra in och ut ur Biotestsj\"on togs bort. Detta syns som en \"okad invandringav lekfisk i Biotestsj\"on och en tillkomst av arter som inte funnits d\"ar p\aa l\"ange. Samtidigt ses f\"or\"andrade tillv\"axtm\"onster hos abborre i Biotestsj\"onsamt utanf\"or, i Forsmarks sk\"arg\aa rd. I viss m\aa n ses \"aven f\"or\"andradeutbredningsm\"onster hos sj\"of\aa gel som skulle kunna kopplas till f\"or\"andradf\"odotillg\aa ng. F\"or bottenfauna kan man inte avg\"ora om det finns m\"onster, p\aa grund av avsaknad av data under senare \aa r.Resultaten antyder att en st\"orre del av fiskbest\aa nden i Forsmarks sk\"arg\aa rd\"an tidigare \"ar beroende av att Biotestsj\"on \"ar en fungerande milj\"o f\"orreproduktion och tillv\"axt. Detta \"ar positivt s\aa{} l\"ange f\"orh\aa llandena iBiotestsj\"on och \"ovriga omr\aa den som p\aa verkas av kylvattenutsl\"app \"argynnsamma, men negativt f\"or omgivande sk\"arg\aa rd om rekryteringen i dessaomr\aa den inte skulle fungera, eller om fiskens h\"alsa skulle p\aa verkas. Dessaaspekter blir s\"arskilt aktuella i och med den planerade effekth\"ojningen vidForsmarks k\"arnkraftverk, som sannolikt skulle kunna accentuera deobserverade effekterna, samt den planerade byggnaden av ett slutf\"orvar, om denna p\aa verkar tillg\aa ngen p\aa{} alternativa rekryteringsmilj\"oer f\"or fisk in\"aromr\aa det.D\"odligheten i silstationen \"ar fortsatt h\"og, om \"an med stormellan\aa rsvariation i antal och artsammans\"attning. M\"atningar vid silstationenvisar dock p\aa{} en \"okad d\"odlighet av \aa l, som inte kan f\"orklaras av generellauppg\aa ngar i best\aa ndet. \"Okningen \"ar troligen en effekt av att mer \aa l \"antidigare vistas i Forsmarks sk\"arg\aa rd, vilket skulle kunna bero p\aa{} enanlockning av \aa l till omr\aa det f\"or kylvattenutsl\"app.En annan f\"or\"andring som intr\"affat under senare \aa r \"ar att en nyintroducerad art, musslan Mytilopsis leucopheata, har observerats i omr\aa detsedan 2011. Arten har orsakat problem i kylvattenv\"agarna i andrak\"arnkraftverk i \"Ostersj\"on, och omr\aa den med f\"orh\"ojd vattentemperatur hartidigare identifierats som potentiella plattformar f\"or fortsatt etablering avarten i andra omr\aa den.},
  isbn = {9789157691842},
  langid = {swedish},
  file = {files/2791/Adill et al. - 2013 - Biologisk recipientkontroll vid Forsmarks kärnkraf.pdf;files/2792/11349.html}
}

@article{agutterMetabolicScalingConsensus2004,
  title = {Metabolic Scaling: Consensus or Controversy?},
  author = {Agutter, P S and Wheatley, D N},
  year = {2004},
  journal = {Theoretical Biology and Medical Modelling},
  volume = {1},
  pages = {13},
  doi = {10.1186/1742-4682-1-13},
  abstract = {BACKGROUND: The relationship between body mass (M) and standard metabolic rate (B) among living organisms remains controversial, though it is widely accepted that in many cases B is approximately proportional to the three-quarters power of M. RESULTS: The biological significance of the straight-line plots obtained over wide ranges of species when B is plotted against log M remains a matter of debate. In this article we review the values ascribed to the gradients of such graphs (typically 0.75, according to the majority view), and we assess various attempts to explain the allometric power-law phenomenon, placing emphasis on the most recent publications. CONCLUSION: Although many of the models that have been advanced have significant attractions, none can be accepted without serious reservations, and the possibility that no one model can fit all cases has to be more seriously entertained.},
  pmid = {15546492},
  keywords = {*Body Weight,*Metabolism,*Models,Animals,Biological,Biometry/*methods,Consensus,Dissent and Disputes,Humans},
  note = {Agutter, Paul S Wheatley, Denys N eng Review England 2004/11/18 09:00 Theor Biol Med Model. 2004 Nov 16;1:13.},
  file = {files/2725/Sherwood et al 2002 Shifting Gears.pdf;files/2726/Chapter 6_Link&Sherwood_Final Edit.pdf;files/2727/Rowan & Rasmussen 1996 Fish activity and cesium.pdf;files/2728/Sherwood et al 2007 Variable cod diet and condition.pdf;files/2729/Trudel et al 2000 Food consumption and mercury.pdf}
}


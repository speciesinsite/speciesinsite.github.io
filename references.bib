@article{kalinkatBodyMassesFunctional2013,
  title = {Body Masses, Functional Responses and Predator-Prey Stability},
  author = {Kalinkat, Gregor and Schneider, Florian D. and Digel, Christoph and Guill, Christian and Rall, Bj{\"o}rn C and Brose, U},
  year = {2013},
  journal = {Ecology Letters},
  volume = {16},
  number = {9},
  pages = {1126--1134},
  issn = {14610248},
  doi = {10.1111/ele.12147},
  abstract = {The stability of ecological communities depends strongly on quantitative characteristics of population interactions (type-II vs. type-III functional responses) and the distribution of body masses across species. Until now, these two aspects have almost exclusively been treated separately leaving a substantial gap in our general understanding of food webs. We analysed a large data set of arthropod feeding rates and found that all functional-response parameters depend on the body masses of predator and prey. Thus, we propose generalised functional responses which predict gradual shifts from type-II predation of small predators on equally sized prey to type-III functional-responses of large predators on small prey. Models including these generalised functional responses predict population dynamics and persistence only depending on predator and prey body masses, and we show that these predictions are strongly supported by empirical data on forest soil food webs. These results help unravelling systematic relationships between quantitative population interactions and large-scale community patterns.},
  pmid = {23819684},
  keywords = {Allometric scaling,Body size,Consumer-resource,Ecological modelling,Feeding rate,Food webs,Interaction strength,Metabolic theory}
}

@article{kapurOceanographicFeaturesDelineate2020,
  title = {Oceanographic Features Delineate Growth Zonation in {{Northeast Pacific}} Sablefish},
  author = {Kapur, M. and Haltuch, M. and Connors, B. and Rogers, L. and Berger, A. and Koontz, E. and Cope, J. and Echave, K. and Fenske, K. and Hanselman, D. and Punt, A. E.},
  year = {2020},
  month = feb,
  journal = {Fisheries Research},
  volume = {222},
  pages = {105414},
  issn = {0165-7836},
  doi = {10.1016/j.fishres.2019.105414},
  abstract = {Renewed interest in the estimation of spatial and temporal variation in fish traits, such as body size, is a result of computing advances and the development of spatially-explicit management frameworks. However, many attempts to quantify spatial structure or the distribution of traits utilize a priori approaches, which involve pre-designated geographic regions and thus cannot detect unanticipated spatial patterns. We developed a new, model-based method that uses the first derivative of the spatial smoothing term of a generalized additive model to identify spatial zones of variation in fish length-at-age. We use simulation testing to evaluate the method across a variety of synthetic, stratified age and length datasets, and then apply it to survey data for Northeast Pacific sablefish (Anoplopoma fimbria). Simulation testing illustrates the robustness of the method across a variety of scenarios related to spatially or temporally stratified length-at-age data, including strict boundaries, overlapping zones and changes at the extreme of the range. Results indicate that length-at-age for Northeast Pacific sablefish increases with latitude, which is consistent with previous work from the western United States. Model-detected spatial breakpoints corresponded to major oceanographic features, including the northern end of the Southern California Bight and the bifurcation of the North Pacific Current. This method has the potential to improve detection of large-scale patterns in fish growth, and aid in the development of spatiotemporally structured population dynamics models to inform ecosystem-based fisheries management.},
  langid = {english},
  keywords = {Ecosystem-based fisheries management,Growth,Sablefish,Spatiotemporal,Von bertalanffy},
  file = {files/2834/Kapur et al. - 2020 - Oceanographic features delineate growth zonation i.pdf;files/2833/S0165783619302693.html}
}


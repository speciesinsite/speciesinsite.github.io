@article{whalenClimateDrivesGeography2020,
  title = {Climate Drives the Geography of Marine Consumption by Changing Predator Communities},
  author = {Whalen, Matthew A. and Whippo, Ross D. B. and Stachowicz, John J. and York, Paul H. and Aiello, Erin and Alcoverro, Teresa and Altieri, Andrew H. and {Benedetti-Cecchi}, Lisandro and Bertolini, Camilla and Bresch, Midoli and Bulleri, Fabio and Carnell, Paul E. and Cimon, St{\'e}phanie and Connolly, Rod M. and Cusson, Mathieu and Diskin, Meredith S. and D'Souza, Elrika and Flores, Augusto A. V. and Fodrie, F. Joel and Galloway, Aaron W. E. and Gaskins, Leo C. and Graham, Olivia J. and Hanley, Torrance C. and Henderson, Christopher J. and Hereu, Clara M. and {Hessing-Lewis}, Margot and Hovel, Kevin A. and Hughes, Brent B. and Hughes, A. Randall and Hultgren, Kristin M. and J{\"a}nes, Holger and Janiak, Dean S. and Johnston, Lane N. and Jorgensen, Pablo and Kelaher, Brendan P. and Kruschel, Claudia and Lanham, Brendan S. and Lee, Kun-Seop and Lefcheck, Jonathan S. and {Lozano-{\'A}lvarez}, Enrique and Macreadie, Peter I. and Monteith, Zachary L. and O'Connor, Nessa E. and Olds, Andrew D. and O'Leary, Jennifer K. and Patrick, Christopher J. and Pino, Oscar and Poore, Alistair G. B. and Rasheed, Michael A. and Raymond, Wendel W. and Reiss, Katrin and Rhoades, O. Kennedy and Robinson, Max T. and Ross, Paige G. and Rossi, Francesca and Schlacher, Thomas A. and Seemann, Janina and Silliman, Brian R. and Smee, Delbert L. and Thiel, Martin and Unsworth, Richard K. F. and van Tussenbroek, Brigitta I. and Verg{\'e}s, Adriana and Yeager, Mallarie E. and Yednock, Bree K. and Ziegler, Shelby L. and Duffy, J. Emmett},
  year = {2020},
  month = oct,
  journal = {PNAS},
  publisher = {{National Academy of Sciences}},
  issn = {0027-8424, 1091-6490},
  doi = {10.1073/pnas.2005255117},
  abstract = {The global distribution of primary production and consumption by humans (fisheries) is well-documented, but we have no map linking the central ecological process of consumption within food webs to temperature and other ecological drivers. Using standardized assays that span 105\textdegree{} of latitude on four continents, we show that rates of bait consumption by generalist predators in shallow marine ecosystems are tightly linked to both temperature and the composition of consumer assemblages. Unexpectedly, rates of consumption peaked at midlatitudes (25 to 35\textdegree ) in both Northern and Southern Hemispheres across both seagrass and unvegetated sediment habitats. This pattern contrasts with terrestrial systems, where biotic interactions reportedly weaken away from the equator, but it parallels an emerging pattern of a subtropical peak in marine biodiversity. The higher consumption at midlatitudes was closely related to the type of consumers present, which explained rates of consumption better than consumer density, biomass, species diversity, or habitat. Indeed, the apparent effect of temperature on consumption was mostly driven by temperature-associated turnover in consumer community composition. Our findings reinforce the key influence of climate warming on altered species composition and highlight its implications for the functioning of Earth's ecosystems.},
  chapter = {Biological Sciences},
  copyright = {\textcopyright{} 2020 . https://www.pnas.org/site/aboutpnas/licenses.xhtmlPublished under the PNAS license.},
  langid = {english},
  keywords = {biogeography,latitudinal gradients,macroecology,seagrass,trophic processes},
  file = {files/2331/Whalen et al. - 2020 - Climate drives the geography of marine consumption.pdf;files/2332/2005255117.html}
}


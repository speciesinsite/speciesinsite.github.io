@article{mionHistoricalGrowthEastern2020,
  title = {Historical Growth of {{Eastern Baltic}} Cod ({{Gadus}} Morhua): {{Setting}} a Baseline with International Tagging Data},
  shorttitle = {Historical Growth of {{Eastern Baltic}} Cod ({{Gadus}} Morhua)},
  author = {Mion, Monica and Hilvarsson, Annelie and H{\"u}ssy, Karin and Krumme, Uwe and {Kr{\"u}ger-Johnsen}, Maria and McQueen, Kate and Mohamed, Esha and Motyka, Roman and Orio, Alessandro and Plikshs, Maris and Radtke, Krzysztof and Casini, Michele},
  year = {2020},
  month = mar,
  journal = {Fisheries Research},
  volume = {223},
  pages = {105442},
  issn = {01657836},
  doi = {10.1016/j.fishres.2019.105442},
  abstract = {Understanding the growth of commercially exploited fish is crucial in fisheries biology and correct estimations of growth and its change over time are paramount for the evaluation of stock status development. Mark-recapture experiments represent a reliable method to estimate growth when age determination based on otolith reading is uncertain, as is the case of the Eastern Baltic cod stock. In this study, historical data (1955\textendash 1970) from tagging experiments on Eastern Baltic cod performed by Sweden, Poland, Denmark, Latvia and Germany were digitised and collated for the first time in a unique dataset to estimate historical von Bertalanffy growth function (VBGF) parameters based on fish length increments using GROTAG model.},
  langid = {english},
  file = {files/2383/Mion et al. - 2020 - Historical growth of Eastern Baltic cod (Gadus mor.pdf}
}

@article{mionMultidecadalChangesFish2021,
  title = {Multidecadal Changes in Fish Growth Rates Estimated from Tagging Data: {{A}} Case Study from the {{Eastern Baltic}} Cod {{{\emph{Gadus}}}}{\emph{ Morhua}}, {{Gadidae}})},
  shorttitle = {Multidecadal Changes in Fish Growth Rates Estimated from Tagging Data},
  author = {Mion, Monica and Haase, Stefanie and Hemmer-Hansen, Jakob and Hilvarsson, Annelie and H{\"u}ssy, Karin and Kr{\"u}ger-Johnsen, Maria and Krumme, Uwe and McQueen, Kate and Plikshs, Maris and Radtke, Krzysztof and Schade, Franziska Maria and Vitale, Francesca and Casini, Michele},
  year = {2021},
  journal = {Fish and Fisheries},
  volume = {22},
  number = {2},
  pages = {413--427},
  issn = {1467-2979},
  doi = {10.1111/faf.12527},
  abstract = {Long time series of reliable individual growth estimates are crucial for understanding the status of a fish stock and deciding upon appropriate management. Tagging data provide valuable information about fish growth, and are especially useful when age-based growth estimates and stock assessments are compromised by age-determination uncertainties. However, in the literature there is a lack of studies assessing possible changes in growth over time using tagging data. Here, data from tagging experiments performed in the Baltic Sea between 1971 and 2019 were added to those previously analysed for 1955\textendash 1970 to build the most extensive tagging dataset available for Eastern Baltic cod (Gadus morhua, Gadidae), a threatened stock with severe age-determination problems. Two length-based methods, the GROTAG model (based on the von Bertalanffy growth function) and a Generalized Additive Model, were used to assess for the first time the potential long-term changes in cod growth using age-independent data. Both methods showed strong changes in growth with an increase until the end of the 1980s (8.6\textendash 10.6 cm/year for a 40 cm cod depending on the model) followed by a sharp decline. This study also revealed that the current growth of cod is the lowest observed in the past 7 decades (4.3\textendash 5.1 cm/year for a 40 cm cod depending on the model), indicating very low productivity. This study provides the first example of the use of tagging data to estimate multidecadal changes in growth rates in wild fish. This methodology can also be applied to other species, especially in those cases where severe age-determination problems exist.},
  copyright = {\textcopyright{} 2020 John Wiley \& Sons Ltd},
  langid = {english},
  keywords = {Baltic cod,generalized additive model,growth modelling,mark-recapture,time series,von Bertalanffy growth function},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/faf.12527},
  file = {files/2698/Mion et al. - 2021 - Multidecadal changes in fish growth rates estimate.pdf;files/2699/faf.html}
}

@article{mionNewPerspectivesEastern2022a,
  title = {New Perspectives on {{Eastern Baltic}} Cod Movement Patterns from Historical and Contemporary Tagging Data},
  author = {Mion, M and Griffiths, Ca and Bartolino, V and Haase, S and Hilvarsson, A and H{\"u}ssy, K and {Kr{\"u}ger-Johnsen}, M and Krumme, U and Lundgreen, Rbc and L{\"o}vgren, J and McQueen, K and Plikshs, M and Radtke, K and Raitaniemi, J and Casini, M},
  year = {2022},
  month = may,
  journal = {Mar. Ecol. Prog. Ser.},
  volume = {689},
  pages = {109--126},
  issn = {0171-8630, 1616-1599},
  doi = {10.3354/meps14047},
  abstract = {Knowledge of the movement patterns and area utilisation of commercially important fish stocks is critical to management. The Eastern Baltic cod               Gadus morhua               , one of the most commercially and ecologically important stocks in the Baltic Sea, is currently one of the most severely impacted fish stocks in Europe. During the last 2 decades, this stock has experienced drastic decreases in population size, distributional range, individual growth and body condition, all of which may have affected the movements between different areas of the Baltic Sea. In this study, we investigated the seasonal movement patterns of Eastern Baltic cod by re-analysing historical tagging data collected by the countries surrounding the Baltic Sea (1955-1988) and compared historical patterns with contemporary data from a recent international tagging experiment (2016-2019). Our re-analyses of historical data showed the presence of different movement behaviours, i.e. resident or seasonally migratory, with larger distances moved by cod released in the northern and central Baltic areas compared to cod released in the southern Baltic areas. Furthermore, trends from the recent tagging experiment indicate a persistent resident strategy in the southern Baltic area. These findings present additional information on general movement patterns and area utilisation of Eastern Baltic cod that could inform future management actions and aid stock recovery.},
  langid = {english},
  file = {files/3885/Mion et al. - 2022 - New perspectives on Eastern Baltic cod movement pa.pdf}
}

@article{mitamuraOccurrenceTemperateCoastal2020,
  title = {Occurrence of a Temperate Coastal Flatfish, the Marbled Flounder {{Pseudopleuronectes}} Yokohamae, at High Water Temperatures in a Shallow Bay in Summer Detected by Acoustic Telemetry},
  author = {Mitamura, Hiromichi and Arai, Nobuaki and Hori, Masakazu and Uchida, Keiichi and Kajiyama, Makoto and Ishii, Mitsuhiro},
  year = {2020},
  month = jan,
  journal = {Fish Sci},
  volume = {86},
  number = {1},
  pages = {77--85},
  issn = {1444-2906},
  doi = {10.1007/s12562-019-01384-2},
  abstract = {The marbled flounder Pseudopleuronectes yokohamae, a flatfish found in temperate coastal waters, is an important commercial species in the northwestern Pacific Ocean, but little is known of its distribution and movement under high water temperatures in summer. We recorded the distribution and movement of marbled flounder in shallow water in Tokyo Bay with acoustic telemetry. Seventeen wild flounder tagged with acoustic transmitters were tracked/monitored by active tracking using a research vessel and sporadic monitoring using fishing gear and a fishing boat. We located six individuals in shallow ({$<$} 30 m) water at temperatures of 19\textendash 23 \textdegree C and dissolved oxygen (DO) concentrations of 3\textendash 5 ml/l in Tokyo Bay, while bottom water temperatures and DO concentrations ranged from 19 to 24 \textdegree C and 0\textendash 7 ml/l, respectively. Our results indicated that at least 35\% of the flounder remained in warm, shallow waters in summer. Multiple monitoring methods and the assistance of fishermen helped to overcome the difficulties of working in a crowded bay and increased our recovery rate of the tagged flounder.},
  langid = {english},
  keywords = {Acoustic transmitter,Active tracking,Biotelemetry,Dissolved oxygen}
}


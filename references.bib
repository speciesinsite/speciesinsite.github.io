@article{peralta-maraverHeatToleranceEctotherms2021,
  title = {Heat Tolerance in Ectotherms Scales Predictably with Body Size},
  author = {{Peralta-Maraver}, Ignacio and Rezende, Enrico L.},
  year = {2021},
  month = jan,
  journal = {Nat. Clim. Chang.},
  volume = {11},
  number = {1},
  pages = {58--63},
  publisher = {{Nature Publishing Group}},
  issn = {1758-6798},
  doi = {10.1038/s41558-020-00938-y},
  abstract = {Recent studies suggest that animals are decreasing in size as a general response to global warming, for reasons that remain unclear. Here, by analysing ectotherm death time curves that take into consideration the intensity and duration of a thermal challenge, we show that heat tolerance varies predictably with size. Smaller animals can maintain higher body temperatures than larger ones during short periods, but cannot maintain higher body temperatures over long periods as their endurance declines more rapidly with time. Body size effects and adaptive variation in heat tolerance may have been obscured in the past by these unaccounted for temporal effects. With increasing size, thermal death occurs at relatively lower metabolic rates with respect to rest at a non-stressful temperature, which might partly explain the reported reductions in organism size with climate warming and shed light on the mechanisms that underlie scaling.},
  copyright = {2020 The Author(s), under exclusive licence to Springer Nature Limited},
  langid = {english},
  annotation = {Bandiera\_abtest: a Cg\_type: Nature Research Journals Primary\_atype: Research Subject\_term: Climate-change ecology;Ecophysiology Subject\_term\_id: climate-change-ecology;ecophysiology},

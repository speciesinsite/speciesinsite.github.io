@article{smolinskiCenturylongCodOtolith2020,
  title = {Century-Long Cod Otolith Biochronology Reveals Individual Growth Plasticity in Response to Temperature},
  author = {Smoli{\'n}ski, Szymon and {Deplanque-Lasserre}, Julie and Hj{\"o}rleifsson, Einar and Geffen, Audrey J. and Godiksen, Jane A. and Campana, Steven E.},
  year = {2020},
  month = oct,
  journal = {Sci Rep},
  volume = {10},
  number = {1},
  pages = {16708},
  publisher = {{Nature Publishing Group}},
  issn = {2045-2322},
  doi = {10.1038/s41598-020-73652-6},
  abstract = {Otolith biochronologies combine growth records from individual fish to produce long-term growth sequences, which can help to disentangle individual from population-level responses to environmental variability. This study assessed individual thermal plasticity of Atlantic cod (Gadus morhua) growth in Icelandic waters based on measurements of otolith increments. We applied linear mixed-effects models and developed a century-long growth biochronology (1908\textendash 2014). We demonstrated interannual and cohort-specific changes in the growth of Icelandic cod over the last century which were mainly driven by temperature variation. Temperature had contrasting relationships with growth\textemdash positive for the fish during the youngest ages and negative during the oldest ages. We decomposed the effects of temperature on growth observed at the population level into within-individual effects and among-individual effects and detected significant individual variation in the thermal plasticity of growth. Variance in the individual plasticity differed across cohorts and may be related to the mean environmental conditions experienced by the group. Our results underscore the complexity of the relationships between climatic conditions and the growth of fish at both the population and individual level, and highlight the need to distinguish between average population responses and growth plasticity of the individuals for accurate growth predictions.},
  copyright = {2020 The Author(s)},
  langid = {english},
  keywords = {Climate-change ecology,Ecology,Marine biology},
  file = {files/3959/Smoliński et al. - 2020 - Century-long cod otolith biochronology reveals ind.pdf;files/3960/s41598-020-73652-6.html}
}


  title = {Intraspecific Phenotypic Variation among Alewife Populations Drives Parallel Phenotypic Shifts in Bluegill},
  author = {Huss, M and Howeth, J G and Osterman, J I and Post, D M},
  year = {2014},
  journal = {Proceedings of the Royal Society of London B: Biological Sciences},

@article{neuheimerClimateEffectsSizeatage2012a,
  title = {Climate Effects on Size-at-Age: Growth in Warming Waters Compensates for Earlier Maturity in an Exploited Marine Fish},
  shorttitle = {Climate Effects on Size-at-Age},
  author = {Neuheimer, Anna B. and Gr{\o}nkj{\ae}r, Peter},

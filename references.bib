@article{arranzDensitydependentEffectsKey2016,
  title = {Density-Dependent Effects as Key Drivers of Intraspecific Size Structure of Six Abundant Fish Species in Lakes across {{Europe}} \textbackslash textlesssup\textbackslash textgreater1\textbackslash textless/Sup\textbackslash textgreater},
  author = {Arranz, Ignasi and Mehner, Thomas and Benejam, Llu{\'i}s and Argillier, Christine and Holmgren, Kerstin and Jeppesen, Erik and Lauridsen, Torben L. and Volta, Pietro and Winfield, Ian J. and Brucet, Sandra and Shuter, Brian},
  year = {2016},
  journal = {Canadian Journal of Fisheries and Aquatic Sciences},
  volume = {73},
  number = {4},
  pages = {519--534},
  issn = {0706-652X},
  doi = {10.1139/cjfas-2014-0508},
  abstract = {We studied fish size structure by using mean size, size diversity, and the slope of linear size spectra of six common European fish species along large-scale environmental gradients. We further analyzed the response of these three size metrics to environmental variables and to density-dependent effects, i.e., relative estimates of abundance (catch per unit effort, CPUE). We found differences in the strength of main predictors of size structure between the six species, but the direction of the response was relatively similar and consistent for most of the size metrics. Mean body size was negatively related to temperature for perch (Perca fluviatilis), roach (Rutilus rutilus), and ruffe (Gymnocephalus cernuus). Lake productivity (expressed as total phos- phorus concentration) and lake depth were also predictors of size structure for four of six species. Moreover, we found a strong density dependence of size structure for all species, resulting in lower mean body size and size diversity and steeper size spectra slopes when density dependence increases. This suggests that density dependence is a key driver of fish size structure.}
}


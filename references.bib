@article{strammaExpandingOxygenminimumZones2008,
  title = {Expanding Oxygen-Minimum Zones in the Tropical Oceans},
  author = {Stramma, Lothar and Johnson, Gregory C. and Sprintall, Janet and Mohrholz, Volker},

@article{heutonOxygenConsumptionDesert2018,
  title = {Oxygen Consumption of Desert Pupfish at Ecologically Relevant Temperatures Suggests a Significant Role for Anaerobic Metabolism},
  author = {Heuton, Matt and Ayala, Luis and Morante, Aldo and Dayton, Kyle and Jones, Alexander C. and Hunt, Joseph R. and McKenna, Austin and {van Breukelen}, Frank and Hillyard, Stanley},
  year = {2018},
  month = sep,
  journal = {J Comp Physiol B},
  volume = {188},
  number = {5},
  pages = {821--830},
  issn = {0174-1578, 1432-136X},
  doi = {10.1007/s00360-018-1174-1},
  abstract = {Oxygen consumption is oftentimes used as a proxy for metabolic rate. However, pupfish acclimated to ecologically relevant temperatures may employ extended periods of anaerobism despite the availability of oxygen\textemdash a process we called paradoxical anaerobism. In this study, we evaluated data from pupfish exhibiting stable oxygen consumption. Routine oxygen consumption ({$\mkern1mu$}RVO2{$\mkern1mu$}) of a refuge population derived from Cyprinodon spp. acclimated to 28 and 33 \textdegree C was evaluated at the ecologically relevant assay temperatures of between 25 and 38 \textdegree C. Different interpretations of the data are available depending on normalization. For instance, RVO2 of smaller fish, measured per fish, was remarkably stable over a wide range of assay temperatures and was not different between acclimation groups. However, when measured on a mass-specific basis, RVO2 in these same smaller fish increases more predictably as temperature increased. RVO2 of refuge fish and the closely related pupfish, C. nevadensis mionectes, measured near their respective acclimation temperatures, were essentially identical. However, RVO2 of 28 \textdegree C acclimated fish of both species, when measured at 34 \textdegree C, was greater than that of the 33 \textdegree C acclimated fish measured at 28 \textdegree C. We suggest that this observed `efficiency' may result from significant anaerobic metabolism use. Experiments investigating factorial aerobic scope ({$\mkern1mu$}MaxVO2/RVO2{$\mkern1mu$}) yielded values less than 1 in 21\textendash 36\% of the 33 \textdegree C acclimated fish. These values indicate a substantial contribution of anaerobic metabolism to energy utilization by these fish. However, muscle lactate levels are not elevated in exercising fish\textemdash a result that is consistent with paradoxical anaerobism use.},
  langid = {english},
  file = {files/2099/Heuton et al. - 2018 - Oxygen consumption of desert pupfish at ecological.pdf}
}


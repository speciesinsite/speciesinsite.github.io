@article{horneRapidShiftsThermal2019,
  title = {Rapid Shifts in the Thermal Sensitivity of Growth but Not Development Rate Causes Temperature\textendash Size Response Variability during Ontogeny in Arthropods},
  author = {Horne, Curtis R. and Hirst, Andrew G. and Atkinson, David and Almeda, Rodrigo and Ki{\o}rboe, Thomas},
  year = {2019},
  journal = {Oikos},
  volume = {128},
  number = {6},
  pages = {823--835},
  issn = {1600-0706},
  doi = {10.1111/oik.06016},
  abstract = {Size at maturity in ectotherms commonly declines with warming. This near-universal phenomenon, formalised as the temperature\textendash size rule, has been observed in over 80\% of tested species, from bacteria to fish. The proximate cause has been attributed to the greater temperature dependence of development rate than growth rate, causing individuals to develop earlier but mature smaller in the warm. However, few studies have examined the ontogenetic progression of the temperature\textendash size response at high resolution. Using marine planktonic copepods, we experimentally determined the progression of the temperature\textendash size response over ontogeny. Temperature\textendash size responses were not generated gradually from egg to adult, contrary to the predictions of a na\"ive model in which development rate was assumed to be more temperature-dependent than growth rate, and the difference in the temperature dependence of these two rates remained constant over ontogeny. Instead, the ontogenetic progression of the temperature\textendash size response in experimental animals was highly episodic, indicating rapid changes in the extent to which growth and development rates are thermally decoupled. The strongest temperature\textendash size responses occurred temporally mid-way through ontogeny, corresponding with the point at which individuals reached between 5 and 25\% of their adult mass. Using the copepod Oithona nana, we show that the temperature-dependence of growth rate varied substantially throughout ontogeny, whereas the temperature dependence of development rate remained constant. The temperature-dependence of growth rate even exceeded that of development rate in some life stages, leading to a weakening of the temperature\textendash size response. Our analyses of arthropod temperature\textendash size responses from the literature, including crustaceans and insects, support these conclusions more broadly. Overall, our findings provide a better understanding of how the temperature\textendash size rule is produced over ontogeny. Whereas we find support for the generality of developmental rate isomorphy in arthropods (shared temperature dependence of development rate across life stages), this concept appears not to apply to growth rates.},
  copyright = {\textcopyright{} 2019 The Authors},
  langid = {english},
  keywords = {body size,plasticity,warming}
}

@article{horneTemperaturesizeResponsesMatch2015,
  title = {Temperature-Size Responses Match Latitudinal-Size Clines in Arthropods, Revealing Critical Differences between Aquatic and Terrestrial Species},
  author = {Horne, Curtis R. and Hirst, Andrew. G. and Atkinson, David},
  editor = {Enquist, Brian},
  year = {2015},
  month = apr,
  journal = {Ecology Letters},
  volume = {18},
  number = {4},
  pages = {327--335},
  issn = {1461023X},
  doi = {10.1111/ele.12413},
  abstract = {Two major intraspecific patterns of adult size variation are plastic temperature-size (T-S) responses and latitude-size (L-S) clines. Yet, the degree to which these co-vary and share explanatory mechanisms has not been systematically evaluated. We present the largest quantitative comparison of these gradients to date, and find that their direction and magnitude co-vary among 12 arthropod orders (r2 = 0.72). Body size in aquatic species generally reduces with both warming and decreasing latitude, whereas terrestrial species have much reduced and even opposite gradients. These patterns support the prediction that oxygen limitation is a major controlling factor in water, but not in air. Furthermore, voltinism explains much of the variation in T-S and L-S patterns in terrestrial but not aquatic species. While body size decreases with warming and with decreasing latitude in multivoltine terrestrial arthropods, size increases on average in univoltine species, consistent with predictions from size vs. season-length trade-offs.},
  langid = {english},
  file = {files/1573/Horne et al. - 2015 - Temperature-size responses match latitudinal-size .pdf}
}


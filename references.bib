@article{heatherGloballyConsistentReef,
  title = {Globally Consistent Reef Size Spectra Integrating Fishes and Invertebrates},
  author = {Heather, Freddie J. and Blanchard, Julia L. and Edgar, Graham J. and Trebilco, Rowan and Stuart-Smith, Rick D.},
  journal = {Ecology Letters},
  volume = {n/a},
  number = {n/a},
  issn = {1461-0248},
  doi = {10.1111/ele.13661},
  abstract = {The frequency distribution of individual body sizes in animal communities (i.e. the size spectrum) provides powerful insights for understanding the energy flux through food webs. However, studies of size spectra in rocky and coral reef communities typically focus only on fishes or invertebrates due to taxonomic and data constraints, and consequently ignore energy pathways involving the full range of macroscopic consumer taxa. We analyse size spectra with co-located fish and mobile macroinvertebrate data from 3369 reef sites worldwide, specifically focusing on how the addition of invertebrate data alters patterns. The inclusion of invertebrates steepens the size spectrum, more so in temperate regions, resulting in a consistent size spectrum slope across latitudes, and bringing slopes closer to theoretical expectations based on energy flow through the system. These results highlight the importance of understanding contributions of both invertebrates and fishes to reef food webs worldwide.},
  copyright = {\textcopyright{} 2020 John Wiley \& Sons Ltd/CNRS},
  langid = {english},
  keywords = {Benthic,biomass equivalence rule,coastal,community,fish,macroecology,size distribution,size spectrum,trophic pathways},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/ele.13661},
  file = {files/2499/Heather et al. - Globally consistent reef size spectra integrating .pdf;files/2500/ele.html}
}


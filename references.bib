@article{neumannOpticalModelBaltic2021,
  title = {Optical Model for the {{Baltic Sea}} with an Explicit {{CDOM}} State Variable: A Case Study with {{Model ERGOM}} (Version 1.2)},
  shorttitle = {Optical Model for the {{Baltic Sea}} with an Explicit {{CDOM}} State Variable},
  author = {Neumann, Thomas and Koponen, Sampsa and Attila, Jenni and Brockmann, Carsten and Kallio, Kari and Kervinen, Mikko and Mazeran, Constant and M{\"u}ller, Dagmar and Philipson, Petra and Thulin, Susanne and V{\"a}kev{\"a}, Sakari and Yl{\"o}stalo, Pasi},

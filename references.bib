@article{tomalaEvaluacionTasaConsumo2014,
  title = {{Evaluacion de la tasa de consumo de oxigeno de \emph{Colossoma macropomum} en relacion al peso corporal y temperatura del agua}},
  author = {Tomala, Dennis and Chavarria, Johnny and Angeles, Beatriz},
  year = {2014},
  month = nov,
  journal = {lajar},
  volume = {42},
  number = {5},
  pages = {971--979},
  issn = {0718560X, 0718560X},
  doi = {10.3856/vol42-issue5-fulltext-4},
  abstract = {Colossoma macropomum is a species in the Amazon region pertaining to the greatest diversity of freshwater fish family Characidae. At the laboratory level, the rate of oxygen consumption of (CO) was determined in a routine metabolism at different body weights: 40, 60, 90, 140 and 250 g (expressed per unit of body mass) and water temperature of 21, 26 and 31\textdegree C. The experiment was design as a completely randomized factorial arrangement (5x3x3), using a respirometer of 20 L and a water recirculation pump. CO measurements were recorded every 5 min by using a digital oximeter. It was shown that the CO maintains interaction with body weight (P) and water temperature (T) (P {$<$} 0.05). The results indicate that CO remains directly related to water temperature and inversely with body weight of the organisms. The regression between independent variables and CO were adjusted to potential equations: CO = 2844.9 P-0.734, CO = 2555.9 P-0.624 and CO = 1945.4 P-0.507 for 21, 26 y 31\textdegree C respectively; where P expressed in kg, yielding high coefficients of correlation and determination. The resulting equation of the multivariate regression analysis was: CO = 4.839 - (0.706* P) + (9.106* T). The results provide information to estimate the capacity in an aquaculture environment based upon the oxygen demand of C. macropomum, contributing to the development of aquaculture with greater technical support.},
  langid = {spanish},
  file = {files/2118/Tomala et al. - 2014 - Evaluacion de la tasa de consumo de oxigeno de Col.pdf}
}

@article{tomczakReferenceStateStructure2022,
  title = {Reference State, Structure, Regime Shifts, and Regulatory Drivers in a Coastal Sea over the Last Century: {{The Central Baltic Sea}} Case},
  shorttitle = {Reference State, Structure, Regime Shifts, and Regulatory Drivers in a Coastal Sea over the Last Century},
  author = {Tomczak, Maciej T. and {M{\"u}ller-Karulis}, B{\"a}rbel and Blenckner, Thorsten and Ehrnsten, Eva and Eero, Margit and Gustafsson, Bo and Norkko, Alf and Otto, Saskia A. and Timmermann, Karen and Humborg, Christoph},
  year = {2022},
  journal = {Limnology and Oceanography},
  volume = {67},
  number = {S1},
  pages = {S266-S284},
  issn = {1939-5590},
  doi = {10.1002/lno.11975},
  abstract = {The occurrence of regime shifts in marine ecosystems has important implications for environmental legislation that requires setting reference levels and targets of quantitative restoration outcomes. The Baltic Sea ecosystem has undergone large changes in the 20th century related to anthropogenic pressures and climate variability, which have caused ecosystem reorganization. Here, we compiled historical information and identified relationships in our dataset using multivariate statistics and modeling across 31 biotic and abiotic variables from 1925 to 2005 in the Central Baltic Sea. We identified a series of ecosystem regime shifts in the 1930s, 1970s, and at the end of the 1980s/beginning of the 1990s. In the long term, the Central Baltic Sea showed a regime shift from a benthic to pelagic-dominated state. Historically, benthic components played a significant role in trophic transfer, while in the more recent productive system pelagic\textendash benthic coupling was weak and pelagic components dominated. Our analysis shows that for the entire time period, productivity, climate, and hydrography mainly affected the functioning of the food web, whereas fishing became important more recently. Eutrophication had far-reaching direct and indirect impacts from a long-term perspective and changed not only the trophic state of the system but also affected higher trophic levels. Our study also suggests a switch in regulatory drivers from salinity to oxygen. The ``reference ecosystem'' identified in our analysis may guide the establishment of an ecosystem state baseline and threshold values for ecosystem state indicators of the Central Baltic Sea.},
  langid = {english},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/lno.11975},
  file = {files/3512/Tomczak et al. - 2022 - Reference state, structure, regime shifts, and reg.pdf;files/3513/lno.html}
}

@article{tomiyamaOntogeneticChangesOptimal2018,
  title = {Ontogenetic Changes in the Optimal Temperature for Growth of Juvenile Marbled Flounder {{{\emph{Pseudopleuronectes}}}}{\emph{ Yokohamae}}},
  author = {Tomiyama, Takeshi and Kusakabe, Kazushi and Otsuki, Noriko and Yoshida, Yusei and Takahashi, Satoshi and Hata, Masaki and Shoji, Jun and Hori, Masakazu},
  year = {2018},
  month = nov,
  journal = {Journal of Sea Research},
  volume = {141},
  pages = {14--20},
  issn = {13851101},
  doi = {10.1016/j.seares.2018.07.010},
  abstract = {We conducted a laboratory experiment and field collections of juvenile marbled flounder (Pseudopleuronectes yokohamae) to assess growth rates of juveniles. In the laboratory, cultured juveniles of approximately 31 and 46 mm standard length grew fastest at 20 \textdegree C, while juveniles of approximately 21 mm grew faster at 12\textendash 14 \textdegree C than at 20 \textdegree C. This result confirmed that optimal temperature for growth of juveniles changes ontogenetically. Juveniles were also collected from shallow estuaries during April to May, during which the water temperature increased from 13 to 22 \textdegree C. The increase in optimal temperature is advantageous for juveniles, because both juvenile size and ambient temperature increase as the season progresses. Growth rates of juveniles were nearly maximum under given temperatures in the field when assessed from otolith microstructure. Thus, juveniles grow at high rates in the field, although ambient temperatures are not necessarily optimal for growth.},
  langid = {english},
  file = {files/2027/Tomiyama et al. - 2018 - Ontogenetic changes in the optimal temperature for.pdf}
}


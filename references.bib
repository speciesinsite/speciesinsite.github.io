@article{claireauxInfluenceWaterTemperature2000,
  title = {Influence of Water Temperature and Oxygenation on the Aerobic Metabolic Scope of {{Atlantic}} Cod ({{Gadus}} Morhua)},
  author = {Claireaux, G. and Webber, D. M. and Lagard{\`e}re, J. -P. and Kerr, S. R.},
  year = {2000},
  month = dec,
  journal = {Journal of Sea Research},
  volume = {44},
  number = {3},
  pages = {257--265},
  issn = {1385-1101},
  doi = {10.1016/S1385-1101(00)00053-8},
  abstract = {Environmental influences (temperature and oxygenation) on cod metabolism and their impact on the ecology of this species were investigated. Limiting oxygen concentration curves (O2 level ranging between 15 and 100\% air saturation) were established at 2, 5 and 10\textdegree C. The standard metabolic rate (SMR), the maximum metabolic rate and the metabolic scope were then modelled as functions of temperature and/or oxygen saturation. The mean SMR at 2, 5 and 10\textdegree C were 19.8{$\pm$}4.9, 30.8{$\pm$}6.1 and 54.3{$\pm$}4.1mg O2h-1kg-1, respectively. Between 2 and 5\textdegree C, the active metabolic rate of cod almost doubled from 65 to 120mg O2h-1kg-1, to reach 177mg O2h-1kg-1 at 10\textdegree C. In terms of metabolic scope (MS), the temperature rise from 2 to 5\textdegree C resulted in a two-fold increase from 45 to 89mg O2h-1kg-1, with MS reaching 123mg O2h-1kg-1 at 10\textdegree C. Our proposed model describing the impact of temperature and oxygen level provides new insight into the energetic interactions which govern the relationship between Atlantic cod and its environment. We re-examined published experimental and field studies from the angle of the regulation of metabolic power. We suggest that, when faced with heterogeneous or unstable hydrological conditions, cod tend to behaviourally maximise their metabolic scope. Through this adaptive response, fish reduce energy budgeting conflicts and presumably increase the probability of routinely operating away from lethal boundaries.},
  langid = {english},
  keywords = {Atlantic cod,metabolism,oxygen,physiological ecology,temperature},
  file = {files/2596/Claireaux et al. - 2000 - Influence of water temperature and oxygenation on .pdf;files/2597/S1385110100000538.html}
}

@article{claireauxResponsesFishesEnvironmental2016,
  title = {Responses by Fishes to Environmental Hypoxia: Integration through {{Fry}}'s Concept of Aerobic Metabolic Scope},
  shorttitle = {Responses by Fishes to Environmental Hypoxia},
  author = {Claireaux, G. and Chabot, D.},
  year = {2016},
  journal = {Journal of Fish Biology},
  volume = {88},
  number = {1},
  pages = {232--251},
  issn = {1095-8649},
  doi = {10.1111/jfb.12833},
  abstract = {The problem of understanding the effect of the environment on fish activities and performance, in any generalized way, remains intractable. Solving this issue is, however, a key to addressing contemporary environmental concerns. As suggested 20 years ago by W. H. Neill, the authors returned to the drawing board, using as a background the conceptual scheme initially proposed by F. E. J. Fry. They revisited the effect of ambient oxygen availability upon fish metabolism and clarified the definitions of limiting, critical and incipient lethal oxygen (ILO) levels. The concepts of oxy-conformer and oxy-regulator are revisited, and P. W. Hochachka's idea of scope for survival is explored. Finally, how the cardiovascular system contributes to the capacity of fishes to respond to the reduced oxygen availability is considered. Various hands-on recommendations and software (R scripts) are provided for researchers interested in investigating these concepts.},
  copyright = {\textcopyright{} 2016 The Fisheries Society of the British Isles},
  langid = {english},
  keywords = {cumulative oxygen deficit,dissolved oxygen,O2crit,oxy-conformer,oxy-regulator,scope for survival},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/jfb.12833},
  file = {files/2704/Claireaux and Chabot - 2016 - Responses by fishes to environmental hypoxia inte.pdf;files/2705/jfb.html}
}

@article{clarkAerobicScopeMeasurements2013,
  title = {Aerobic Scope Measurements of Fishes in an Era of Climate Change: Respirometry, Relevance and Recommendations},
  author = {Clark, T D and Sandblom, E and Jutfelt, F},
  year = {2013},
  journal = {Journal of Experimental Biology},
  volume = {216},
  number = {Pt 15},
  pages = {2771--2782},
  doi = {10.1242/jeb.084251},
  abstract = {Measurements of aerobic scope [the difference between minimum and maximum oxygen consumption rate ( and , respectively)] are increasing in prevalence as a tool to address questions relating to fish ecology and the effects of climate change. However, there are underlying issues regarding the array of methods used to measure aerobic scope across studies and species. In an attempt to enhance quality control before the diversity of issues becomes too great to remedy, this paper outlines common techniques and pitfalls associated with measurements of , and aerobic scope across species and under different experimental conditions. Additionally, we provide a brief critique of the oxygen- and capacity-limited thermal tolerance (OCLTT) hypothesis, a concept that is intricately dependent on aerobic scope measurements and is spreading wildly throughout the literature despite little evidence for its general applicability. It is the intention of this paper to encourage transparency and accuracy in future studies that measure the aerobic metabolism of fishes, and to highlight the fundamental issues with assuming broad relevance of the OCLTT hypothesis.},
  pmid = {23842625},
  keywords = {*Climate Change,*Respiration,aerobic metabolism,Aerobiosis,Animals,Epoc,excess post-exercise oxygen consumption,Fishes/*physiology,global warming,oxygen consumption rate,oxygen uptake,oxygen- and capacity-limited thermal tolerance,Physiology/*methods,specific dynamic action},
  note = {Clark, Timothy D Sandblom, Erik Jutfelt, Fredrik eng Research Support, Non-U.S. Gov't Review England 2013/07/12 06:00 J Exp Biol. 2013 Aug 1;216(Pt 15):2771-82. doi: 10.1242/jeb.084251.}
}

@article{clarkDynamicGeneralisedAdditive,
  title = {Dynamic Generalised Additive Models ({{DGAMs}}) for Forecasting Discrete Ecological Time Series},
  author = {Clark, Nicholas J. and Wells, Konstans},
  journal = {Methods in Ecology and Evolution},
  volume = {n/a},
